{
  "manifest_version": 3,
  "name": "YouTube Trust Checker",
  "version": "1.0",
  "permissions": ["activeTab", "storage", "https://*.youtube.com/*"],
  "background": {
    "service_worker": "src/background/service.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/watch*"],
      "js": ["src/content/inject.js"]
    }
  ]
}
